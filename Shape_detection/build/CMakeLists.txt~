cmake_minimum_required(VERSION 2.8)
project( houghcircle )
find_package( OpenCV REQUIRED )
add_executable( ../houghcircle ../houghcircle.cpp )
target_link_libraries( houghcircle ${OpenCV_LIBS} )

cmake_minimum_required(VERSION 2.8)
project( misc_shape_detect )
find_package( OpenCV REQUIRED )
add_executable( ../misc_shape_detect ../misc_shape_detect.cpp )
target_link_libraries( misc_shape_detect ${OpenCV_LIBS} )

